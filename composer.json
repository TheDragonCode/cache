{
    "name": "dragon-code/pretty-cache",
    "description": "An improved interface for working with cache",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "cache",
        "helper",
        "pretty",
        "pretty-cache"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/pretty-cache/issues",
        "source": "https://github.com/TheDragonCode/pretty-cache"
    },
    "require": {
        "php": "^7.3|^8.0",
        "dragon-code/contracts": "^2.3.3",
        "dragon-code/support": "^5.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "DragonCode\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
